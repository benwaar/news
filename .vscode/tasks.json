{
  "$schema": "https://json.schemastore.org/tasks.json",
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "start",
      "path": "services/ui-news",
      "label": "ui-news: npm start",
      "problemMatcher": {
        "owner": "custom",
        "fileLocation": "absolute",
        "pattern": {
          "regexp": ".*",
          "file": 1,
          "location": 2,
          "message": 0
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": "Compiled successfully."
        }
      },
      "isBackground": true
    }
  ]
}
